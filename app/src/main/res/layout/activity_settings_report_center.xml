<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/default_bg_color"
    >
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="@dimen/y27"
    android:background="@mipmap/daohang">

    <ImageView
        android:id="@+id/iv_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/iv_back"
        android:paddingLeft="@dimen/x9"
        android:layout_alignParentBottom="true"

        android:paddingBottom="@dimen/x10"
        android:paddingRight="@dimen/x10" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="举报中心"
        android:textSize="@dimen/y7"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="@dimen/x9"
        android:textColor="@color/white"
        />
</RelativeLayout>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/yes"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/x34"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="提交成功"
        android:textSize="@dimen/x10"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/x15"
        />
</LinearLayout>