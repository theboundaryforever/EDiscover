<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/default_bg_color"
    >
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="@dimen/bt_margin_49"
    android:background="@mipmap/daohang">

    <ImageView
        android:id="@+id/iv_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/iv_back"
        android:layout_centerVertical="true"
        android:padding="@dimen/bt_margin_15" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="举报中心"
        android:textSize="@dimen/font17"
        android:layout_centerInParent="true"
        android:textColor="@color/white" />
</RelativeLayout>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/yes"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/bt_margin_57" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="提交成功"
        android:textSize="@dimen/font16"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/bt_margin_25" />
</LinearLayout>